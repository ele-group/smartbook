                                        <ImageViewEx autoSize="false" centered="false" focusTraversable="false" height="432" id="imageLesson_2_illustration_4" layoutX="88" layoutY="240" mosaic="false" mosaicGap="0" proportional="true" stretch="true" styleClass="" textFill="#000000" width="344">
